<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    
    <mapper namespace="com.winter.app.users.UsersDAO">
    	<insert id="usersAdd" parameterType="UsersDTO" >
	    	INSERT INTO users VALUES(#{username}, #{password}, #{name}, #{email}, #{phone}, #{birth});
    	</insert>
    	
    	
    	<select id="mypage" resultType="UsersDTO" parameterType="UsersDTO">
    		SELECT * 
			from users u
			where username=123    		
    	</select>
    	
    </mapper>